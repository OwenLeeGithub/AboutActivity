<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.insanus.course.aboutactivity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:icon="@drawable/activity"
            android:label="配置activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.BundleMutual" />
        <activity android:name=".activities.BundleShow" />
        <activity android:name=".activities.ResultActivity" />
        <activity android:name=".activities.ResultOtherActivity" />
        <activity
            android:name=".activities.launchMode.StandardActivity"
            android:launchMode="standard" />
        <activity android:name=".activities.FragmentActivity" />
        <activity android:name=".activities.BookDetailActivity"/>
    </application>

</manifest>